public class Particle 
{
  private int x;
  private int y;
  private double dx;
  private double dy;
  private int length;
  private int width;
  private int life;
  private String theImage
   
   public Particle(int x, int y, double dx, double dy, int length, int width, int life, String theImage)
   {
    this.x = x;
    this.y = y;
    this.dx = dx;
    this.dy = dy;
    this.length = length;
    this.width = width;
    this.life = life;
    this.theImage = theImage;
    
   }
   
   public boolean update()
   {

   }

   public void render(Graphics g)
   {

   }
}